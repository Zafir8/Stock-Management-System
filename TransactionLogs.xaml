<Window x:Class="SDAM2_Stock_Management_App.TransactionLogs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SDAM2_Stock_Management_App"
        mc:Ignorable="d"
        Title="Stock Management System" Height="650" Width="1000" FontSize="14" Background="#0e0d45" Foreground="#eeeeee">
    <Grid>
        <DataGrid Name="stockDataGrid" AutoGenerateColumns="True" VerticalAlignment="Top" HorizontalAlignment="Center"></DataGrid>
        <Button Content="Back" Width="100" Height="30" VerticalAlignment="Bottom" Margin="10" HorizontalAlignment="Center" Background="#4a90e2" Foreground="#eeeeee" Click="Back_Click"/>
    </Grid>

</Window>
